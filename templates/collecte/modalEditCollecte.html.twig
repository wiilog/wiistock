<form method="" class="modal fade" id="modalEditCollecte" role="dialog" aria-labelledby="dataModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            {% include('utils/modal/header.html.twig') with {
                title: "Modifier une collecte"
            } %}

            <div class="modal-body modal-input container-fluid"></div>
            <div class="error-msg"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">Annuler</button>
                <button id="submitEditCollecte" type="button" name="collecte" class="btn btn-success data">Enregistrer</button>
            </div>
        </div>
    </div>
</form>
