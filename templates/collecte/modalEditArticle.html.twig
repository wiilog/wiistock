<!-- Modale Modification de quantité à collecter d'un article -->
<div method="" class="modal fade" id="modalEditArticle" role="dialog" aria-labelledby="modalModifyArticleTitle"
     aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Modifier la quantité</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body"></div>
            <div class="error-msg"></div>
            <div class="modal-footer">
                <div class="btn" data-dismiss="modal">Annuler</div>
                <div id="submitEditArticle" class="save btn btn-success" name="collecteRef" value="">Valider</div>
            </div>
        </div>
    </div>
</div>
