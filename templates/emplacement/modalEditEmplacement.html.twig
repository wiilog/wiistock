<form method="" class="modal fade" id="modalEditEmplacement" role="dialog" aria-labelledby="dataModalCenterTitle"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            {% include 'utils/modal/header.html.twig' with {
                title: "Modifier un emplacement"
            } %}

            <div class="modal-body modal-input container-fluid">
            </div>
            {% include 'utils/modal/footer.html.twig' with {
                elements: [
                    {classes: 'btn btn-outline-secondary', dismiss: true, text: 'Annuler', event:'clearErrorMsg($(this))'},
                    {classes: 'btn btn-success data', text: 'Enregistrer', name: 'id', id: 'submitEditEmplacement'}
                ],
            } %}

        </div>
    </div>
</form>
