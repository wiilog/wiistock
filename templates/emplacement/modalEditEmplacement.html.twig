<form method="" class="modal fade" id="modalEditEmplacement" role="dialog" aria-labelledby="dataModalCenterTitle"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Modifier un emplacement</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body modal-input container-fluid">
            </div>
            <div class="error-msg"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal" onclick="clearErrorMsg($(this))">Annuler</button>
                <button id="submitEditEmplacement" class="btn btn-success data" type="button"
                    name='id' value=''>Enregistrer</button>
            </div>
        </div>
    </div>
</form>
