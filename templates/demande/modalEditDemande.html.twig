<div class="modal fade" id="modalEditDemande" role="dialog" aria-labelledby="dataModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            {% include 'utils/modal/header.html.twig' with {
                title: "Modifier la demande"
            } %}

            <div class="modal-body modal-input container-fluid">
            </div>
            {% include 'utils/modal/footer.html.twig' with {
                elements: [
                    {input: true, classes: 'data', name: 'demandeId', value: demande.id},
                    {classes: 'btn btn-outline-secondary', dismiss: true, text: 'Annuler'},
                    {classes: 'btn btn-success data', text: 'Enregistrer', type: 'submit'}
                ],
                hideErrorContainer: true,
            } %}

        </div>
    </div>
</div>
