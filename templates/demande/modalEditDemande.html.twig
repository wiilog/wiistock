<form method="" class="modal fade" id="modalEditDemande" role="dialog" aria-labelledby="dataModalCenterTitle"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Modifier la demande</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body modal-input container-fluid">
            </div>
            <div class="modal-footer">
                <input type="hidden" class="data" name="demandeId" value="{{ demande.id }}">
                <button type="button" class="btn" data-dismiss="modal">Annuler</button>
                <button id="submitEditDemande" type="button" class="btn btn-success data">Enregistrer</button>
            </div>
        </div>
    </div>
</form>
