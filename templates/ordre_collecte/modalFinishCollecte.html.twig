<form method="" class="modal fade" id="modalFinishCollecte" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            {% include('utils/modal/header.html.twig') with {
                title: "Choisissez un point de dépose"
            } %}

            <div class="modal-body">
                <table class="table">
                    <thead>
                    <tr>
                        <th>Code barre</th>
                        <th>Point de dépose</th>
                    </tr>
                    </thead>
                    <tbody></tbody>
                </table>
                <div class="error-msg"></div>
                <div class="modal-footer">
                    <button type="button"
                            class="btn btn-outline-secondary"
                            data-dismiss="modal">
                        Annuler
                    </button>
                    <button id="submitFinishCollecte"
                            type="button"
                            class="btn btn-success">
                        Finir la collecte
                    </button>
                </div>
            </div>
        </div>
    </div>
</form>
