<form method="" class="modal fade" id="modalFinishCollecte" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Choisissez un point de dépose</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <table class="table">
                    <thead>
                        <tr>
                            <th>
                                Code barre
                            </th>
                            <th>
                                Point de dépose
                            </th>
                        </tr>
                    </thead>
                    <tbody>

                    </tbody>
                </table>
            <div class="error-msg p-4"></div>
            <div class="modal-footer">
                <button type="button"
                        class="btn"
                        data-dismiss="modal">
                    Annuler
                </button>
                <button id="submitFinishCollecte"
                        type="button"
                        class="btn btn-success">
                    Finir la collecte
                </button>
            </div>
        </div>
    </div>
</form>
