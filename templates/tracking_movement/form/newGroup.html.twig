{% import 'form.html.twig' as form %}

<div class="row">
    <div class="form-group col-12">
        {{ form.input('parent', trans('Traçabilité', 'Mouvements', 'UL contenante', false), true) }}
    </div>

    <div class="form-group col-12">
        {{ form.select('pack', trans('Traçabilité', 'Général', 'Unités logistiques'), true, {
            type: '',
            editableTokenSeparator: ' ',
            multiple: true,
            editable: true,
        }) }}
    </div>

    {{ form.hidden('is-group', 1) }}
</div>
