<form method="" class="modal fade" id="modalDeleteReception" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            {% include('utils/modal/header.html.twig') with {
                title: "Supprimer"~'réception.la réception' | trans
            } %}

            <div class="modal-body">
            </div>
            <div class="modal-footer">
                <input type="hidden" class="data" name="receptionId" value="{{ reception.id }}">
                <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">Fermer</button>
                <button id="submitDeleteReception" type="button" data-dismiss="modal" class="btn btn-danger data">Supprimer</button>
            </div>
        </div>
    </div>
</form>
