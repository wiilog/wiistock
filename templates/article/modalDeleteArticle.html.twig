<form method="" class="modal fade" id="modalDeleteArticle" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            {% include 'utils/modal/header.html.twig' with {
                title: "Supprimer l'article"
            } %}

            <div class="modal-body">
            </div>
            {% include 'utils/modal/footer.html.twig' with {
                elements:
                [
                    {classes: 'btn btn-outline-secondary', dismiss: true, text: 'Annuler'},
                    {id: 'submitDeleteArticle', name:'article', classes: 'btn btn-danger data', dismiss: true, text: 'Supprimer'}
                ],
                hideErrorContainer: true
            } %}
        </div>
    </div>
</form>
