<form method="" class="modal fade" id="modalEditUrgence" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            {% include('utils/modal/header.html.twig') with {
                title: "Modifier une "~'urgences.urgence' | trans
            } %}

            <div class="modal-body modal-input container-fluid"></div>

            <div class="error-msg"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">Annuler</button>
                <button id="submitEditUrgence" type="button" name="id"
                        class="btn btn-success data">Enregistrer</button>
            </div>
        </div>
    </div>
</form>
