<form method="" class="modal fade" id="modalDeletePack" role="dialog" aria-labelledby="dataModalCenterTitle"
      aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            {% include('utils/modal/header.html.twig') with {
                title: 'Supprimer la référence article'
            } %}

            <div class="modal-body">
                Voulez-vous réellement supprimer ce colis ?
            </div>
            <div class="modal-footer">
                <input type="hidden" class="{{ arrivage is defined ? "data" }}" name="arrivage" value="{{ arrivage is defined ? arrivage.id }}">
                <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">Annuler</button>
                <button id="submitDeletePack" name="pack" value="" type="button"
                        class="btn btn-danger data">Supprimer</button>
            </div>
        </div>
    </div>
</form>
