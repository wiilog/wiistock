<form method="" class="modal fade" id="modalDeleteDispatch" role="dialog" aria-labelledby="dataModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            {% include('utils/modal/header.html.twig') with {
                title: "Supprimer la"~'acheminement.demande d\'acheminement'|trans
            } %}

            <div class="modal-body">
                Voulez-vous r√©ellement supprimer cette <span title="demande d'acheminement">{{ 'acheminement.demande d\'acheminement'|trans }}</span> ?
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">Annuler</button>
                <button id="submitDeleteDispatch"
                        name="dispatch"
                        value="{{ dispatch.id }}"
                        type="button"
                        data-dismiss="modal"
                        class="btn btn-danger data">Supprimer</button>
            </div>
        </div>
    </div>
</form>
