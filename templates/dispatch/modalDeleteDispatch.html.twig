<div class="modal fade" id="modalDeleteDispatch" role="dialog" aria-labelledby="dataModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            {% include 'utils/modal/header.html.twig' with {
                title: trans('Demande', 'Acheminements', 'Général', "Supprimer la demande d'acheminement")
            } %}

            <div class="modal-body">
                {{trans('Demande', 'Acheminements', 'Général', "Voulez-vous réellement supprimer cette demande d'acheminement ?" )}}
            </div>
            {% include 'utils/modal/footer.html.twig' with {
                elements: [
                    {classes: 'btn btn-outline-secondary', dismiss: true, text: trans('Général', null, 'Modale', 'Annuler')},
                    {classes: 'btn btn-danger data', text: trans('Général', null, 'Modale', 'Supprimer'), id: 'submitDeleteDispatch', name: 'dispatch', value: dispatch.id, dismiss: true}
                ],
                renderErrorContainer: false
            } %}

        </div>
    </div>
</div>
