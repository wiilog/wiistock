<form method="" class="modal fade" id="modalNewRefArticle" role="dialog" aria-labelledby="dataModalCenterTitle"
      aria-hidden="true">
    <div class="modal-dialog  modal-xl" role="document">
        <div class="modal-content">
            {% include('utils/modal/header.html.twig') with {
                title: "Créer un article de référence"
            } %}

            <div class="modal-body modal-input container-fluid">
                {% include 'reference_article/modalNewRefArticleContent.html.twig' %}
            </div>
            <div class="error-msg"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">Annuler</button>
                <button type="button" id="submitNewRefArticle" class="btn btn-success">Enregistrer</button>
            </div>
        </div>
    </div>
</form>
