<div class="modal fade modalDeleteEntity" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            {% include 'utils/modal/header.html.twig' %}
            <div class="modal-body">
                <div class="spinner-container d-flex justify-content-center">
                    <div class="spinner-border sizeLoader mini-loader" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                </div>
                <div class="message-container d-none"></div>
            </div>
            {% include 'utils/modal/footer.html.twig' with {
                elements: [
                    {classes: 'btn btn-outline-secondary', dismiss: true, text: 'Annuler'},
                    {classes: 'btn btn-danger submit-danger', text: 'Supprimer'},
                ],
                hideErrorContainer: true,
            } %}

        </div>
    </div>
</div>
