<div class="col-auto mb-2 select-filter pr-0">
    <div {{ multiple is defined ? 'class="select2-overflow"' }}>
        <div class="input-group filter-input w-100">
            <select class="form-control data cursor-default filter-select2 ajax-autocomplete-emplacements" name="emplacement"
                    id="emplacement" {{ multiple is defined ? 'multiple' }}
                    {{ disabled }}
                    {{ isPreFilled ? 'disabled' }}>
                {% if isPreFilled %}
                    {% for location in locationsFilter %}
                        <option selected value="{{ location.id }}">{{ location.label }}</option>
                    {% endfor %}
                {% endif %}
            </select>
        </div>
    </div>
</div>
