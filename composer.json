{
    "name": "wiilog/wiistock",
    "type": "project",
    "license": "proprietary",
    "description": "Wiistock web application",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "beberlei/doctrineextensions": "^1.3",
        "composer/semver": "^3.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofsymfony/jsrouting-bundle": "^2.7",
        "friendsofsymfony/rest-bundle": "^3.0",
        "google/apiclient": "^2.10",
        "knplabs/knp-snappy-bundle": "^1.8",
        "nelmio/cors-bundle": "^2.1",
        "nikitospush/barcode-bundle": "^3.0",
        "ovh/ovh": "^3.0",
        "scienta/doctrine-json-functions": "^4.2",
        "sensio/framework-extra-bundle": "^5.6",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/asset": "^5.2",
        "symfony/console": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/dom-crawler": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/expression-language": "^5.2",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-client": "^5.2",
        "symfony/intl": "^5.2",
        "symfony/mailer": "^5.2",
        "symfony/mime": "^5.2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "^5.2",
        "symfony/orm-pack": "^2",
        "symfony/process": "^5.2",
        "symfony/security-bundle": "^5.2",
        "symfony/security-guard": "^5.2",
        "symfony/serializer-pack": "^1.0",
        "symfony/string": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "^5.2",
        "symfony/web-link": "^5.2",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "^5.2",
        "twig/string-extra": "^3.3",
        "wiilog/common": "dev-master"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.25",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "^5.2"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^5.2"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/wiilog/common-php.git"
        }
    ]
}
